{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020",
    ],
    "types": [
      "node",
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": false,
    "inlineSourceMap": true,
    "strict": true,
    "target": "es2019",
    "declaration": false,
    "allowJs": false
  },
  "include": [ "src" ],
  "exclude": []
}
